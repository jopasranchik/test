numbers = [5 , 2 , 7]
#numbers[3] = 100
numbers.append(100) #добавляет значение в конец
numbers.insert(1, True) #добавляет значение на место куда ты его поставишь (место ставиться в начале)
b = [5 , 6 ,8]
numbers.extend(b) #добавляет в конец несколько хначений и более
numbers.sort() #сортирукт по возрастанию
#numbers.reverse() #переворачивает значения
numbers.pop(0) #в поп можно менять индекс
#numbers.remove(2) # удаляет значение которые ты напишешь в скобках
#numbers.clear() #удаляет все
print(numbers)